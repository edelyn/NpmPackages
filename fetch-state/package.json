{
  "name": "@nait-aits/fetch-state",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/edelyn/NpmPackages.git"
  },
  "description": "A library to make loading data, and assigning it to states easier.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "del /Q dist && del *.tgz && npm run build:esm && npm run build:cjs",
    "pack": "npm run build & npm pack",
    "publisher": "npm run build & npm publish --access=public",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "author": "NAIT ITS",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  }
}
